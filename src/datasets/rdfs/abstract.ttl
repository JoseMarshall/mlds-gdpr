rgdpr:cpt_8.art_78.pt_1_eu_it a eli:LegalExpression ;
    eli:description """1.Fatto salvo ogni altro ricorso amministrativo o extragiudiziale, ogni persona fisica o
      giuridica ha il diritto di proporre un ricorso giurisdizionale effettivo avverso una decisione
      giuridicamente vincolante dell'autorità di controllo che la riguarda.""" ;
    eli:has_translation rgdpr:cpt_8.art_78.pt_1_eu_de,
        rgdpr:cpt_8.art_78.pt_1_eu_en,
        rgdpr:cpt_8.art_78.pt_1_eu_pt ;
