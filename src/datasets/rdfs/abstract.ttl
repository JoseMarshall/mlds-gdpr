    eli:realizes gdpr:cpt_6.art_29.pt_4_abstract_pt ;
    eli:title_alternative "Artigo 29.º-4" .

rgdpr:cpt_6.art_29.pt_5_pt a eli:LegalExpression ;
    eli:description """5 - O dever de sigilo referido no número anterior é também aplicável a todos os titulares de
