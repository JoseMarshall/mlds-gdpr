    eli:number "1" ;
    eli:realizes gdpr:cpt_8.art_81.pt_1 ;
    eli:title_alternative "Articolo 81-1" .

rgdpr:cpt_8.art_81.pt_1_eu_pt a eli:LegalExpression ;
    eli:description """1. Caso um tribunal de um Estado-Membro tenha informações sobre um processo pendente num tribunal
  de outro Estado-Membro, relativo ao mesmo assunto no que se refere às atividades de tratamento do
  mesmo responsável pelo tratamento ou subcontratante, deve contactar o referido tribunal desse
  outro Estado-Membro a fim de confirmar a existência de tal processo.""" ;
    eli:has_translation rgdpr:cpt_8.art_81.pt_1_eu_de,
        rgdpr:cpt_8.art_81.pt_1_eu_en,
        rgdpr:cpt_8.art_81.pt_1_eu_it ;
