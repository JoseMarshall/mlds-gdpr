    eli:number "71" ;
    eli:realizes gdpr:p_3.cpt_4.art_71_abstract_de ;
    eli:title_alternative "Artikel 71" .

rgdpr:p_3.cpt_4.art_72.pt_1_de a eli:LegalExpression ;
