        rgdpr:cpt_7.sct_2.art_66.pt_2_eu_en,
        rgdpr:cpt_7.sct_2.art_66.pt_2_eu_pt ;
    eli:number "2" ;
    eli:realizes gdpr:cpt_7.sct_2.art_66.pt_2 ;
    eli:title_alternative "Articolo 66-2" .

rgdpr:cpt_7.sct_2.art_66.pt_2_eu_pt a eli:LegalExpression ;
    eli:description """2. Quando a autoridade de controlo tiver tomado uma medida nos termos do n.o
  1 e considerar necessário adotar urgentemente medidas definitivas, pode solicitar um parecer
  urgente ou uma decisão vinculativa urgente ao Comité, fundamentando o seu pedido de parecer ou
  decisão.""" ;
    eli:has_translation rgdpr:cpt_7.sct_2.art_66.pt_2_eu_de,
