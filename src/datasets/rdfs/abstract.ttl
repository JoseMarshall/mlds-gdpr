    eli:realizes gdpr:p_3.cpt_4.art_65.pt_4_abstract_de ;
    eli:title_alternative "Artikel 65-4" .

rgdpr:p_3.cpt_4.art_65.pt_5_de a eli:LegalExpression ;
    eli:description "Der Verantwortliche hat Verletzungen des Schutzes personenbezogener Daten zu dokumentieren. Die Dokumentation hat alle mit den Vorfällen zusammenhängenden Tatsachen, deren Auswirkungen und die ergriffenen Abhilfemaßnahmen zu umfassen." ;
    eli:number "5" ;
    eli:realizes gdpr:p_3.cpt_4.art_65.pt_5_abstract_de ;
