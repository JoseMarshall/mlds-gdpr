    eli:realizes gdpr:cpt_7.sct_2.art_64.pt_3_eu_pt ;
    eli:title_alternative "Artigo 64.o-3" .

rgdpr:cpt_7.sct_2.art_64.pt_4_eu_de a eli:LegalExpression ;
