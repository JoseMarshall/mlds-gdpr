    eli:realizes gdpr:cpt_4.sct_5.art_42.pt_5 ;
    eli:title_alternative "Artigo 42.o-5" .

rgdpr:cpt_4.sct_5.art_42.pt_6_eu_de a eli:LegalExpression ;
    eli:description """(6)Der Verantwortliche oder der Auftragsverarbeiter, der die von ihm durchgeführte Verarbeitung
  dem Zertifizierungsverfahren unterwirft, stellt der Zertifizierungsstelle nach Artikel 43 oder
  gegebenenfalls der zuständigen Aufsichtsbehörde alle für die Durchführung des
  Zertifizierungsverfahrens erforderlichen Informationen zur Verfügung und gewährt ihr den in diesem
  Zusammenhang erforderlichen Zugang zu seinen Verarbeitungstätigkeiten.""" ;
    eli:has_translation rgdpr:cpt_4.sct_5.art_42.pt_6_eu_en,
        rgdpr:cpt_4.sct_5.art_42.pt_6_eu_it,
        rgdpr:cpt_4.sct_5.art_42.pt_6_eu_pt ;
