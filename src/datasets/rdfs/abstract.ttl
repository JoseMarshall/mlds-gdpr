rgdpr:p_3.cpt_1.art_46.pt_16_de a eli:LegalExpression ;
    eli:description "„internationale Organisation“ eine völkerrechtliche Organisation und ihre nachgeordneten Stellen sowie jede sonstige Einrichtung, die durch eine von zwei oder mehr Staaten geschlossene Übereinkunft oder auf der Grundlage einer solchen Übereinkunft geschaffen wurde;" ;
    eli:number "16" ;
    eli:realizes gdpr:p_3.cpt_1.art_46.pt_16_abstract_de ;
