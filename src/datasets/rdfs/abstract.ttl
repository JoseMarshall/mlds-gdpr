    eli:description "4.Each Member State shall ensure that each supervisory authority is provided with the human, technical and financial resources, premises and infrastructure necessary for the effective performance of its tasks and exercise of its powers, including those to be carried out in the context of mutual assistance, cooperation and participation in the Board." ;
    eli:has_translation rgdpr:cpt_6.sct_1.art_52.pt_4_eu_de,
        rgdpr:cpt_6.sct_1.art_52.pt_4_eu_it,
        rgdpr:cpt_6.sct_1.art_52.pt_4_eu_pt ;
    eli:is_translation_of rgdpr:cpt_6.sct_1.art_52.pt_4_eu_de,
        rgdpr:cpt_6.sct_1.art_52.pt_4_eu_it,
        rgdpr:cpt_6.sct_1.art_52.pt_4_eu_pt ;
    eli:number "4" ;
    eli:realizes gdpr:cpt_6.sct_1.art_52.pt_4 ;
    eli:title_alternative "Article 52-4" .

