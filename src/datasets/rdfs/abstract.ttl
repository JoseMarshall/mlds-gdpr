    eli:number "4" ;
    eli:realizes gdpr:cpt_6.art_31.pt_4_abstract_pt ;
    eli:title_alternative "Artigo 31.º-4" .

rgdpr:cpt_6.art_31.pt_5_pt a eli:LegalExpression ;
