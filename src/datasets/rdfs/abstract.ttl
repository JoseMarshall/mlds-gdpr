    eli:title_alternative "Article 45-2" .

rgdpr:cpt_5.art_45.pt_2_eu_it a eli:LegalExpression ;
    eli:description """2.Nel valutare l'adeguatezza del livello di protezione, la Commissione prende in
      considerazione in particolare i seguenti elementi:""" ;
    eli:has_translation rgdpr:cpt_5.art_45.pt_2_eu_de,
