    eli:realizes gdpr:p_3.cpt_4.art_65.pt_6_abstract_de ;
    eli:title_alternative "Artikel 65-6" .

rgdpr:p_3.cpt_4.art_65.pt_7_de a eli:LegalExpression ;
    eli:description "Absatz 4 findet entsprechende Anwendung." ;
    eli:number "7" ;
    eli:realizes gdpr:p_3.cpt_4.art_65.pt_7_abstract_de ;
    eli:title_alternative "Artikel 65-7" .

rgdpr:p_3.cpt_4.art_65.pt_8_de a eli:LegalExpression ;
    eli:description "Weitere Pflichten des Verantwortlichen zu Benachrichtigungen über Verletzungen des Schutzes personenbezogener Daten bleiben unberührt." ;
    eli:number "8" ;
    eli:realizes gdpr:p_3.cpt_4.art_65.pt_8_abstract_de ;
    eli:title_alternative "Artikel 65-8" .

rgdpr:p_3.cpt_4.art_65_de a eli:LegalExpression ;
