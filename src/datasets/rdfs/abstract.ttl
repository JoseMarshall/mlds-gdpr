rgdpr:cpt_2.art_7.pt_1_eu_de a eli:LegalExpression ;
    eli:description """(1)Beruht die Verarbeitung auf einer Einwilligung, muss der Verantwortliche nachweisen können,
  dass die betroffene Person in die Verarbeitung ihrer personenbezogenen Daten eingewilligt hat.""" ;
    eli:has_translation rgdpr:cpt_2.art_7.pt_1_eu_en,
        rgdpr:cpt_2.art_7.pt_1_eu_it,
        rgdpr:cpt_2.art_7.pt_1_eu_pt ;
