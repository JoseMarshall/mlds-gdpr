    eli:number "1" ;
    eli:realizes gdpr:cpt_6.art_30.pt_1_abstract_pt ;
    eli:title_alternative "Artigo 30.º-1" .

rgdpr:cpt_6.art_30.pt_2_pt a eli:LegalExpression ;
    eli:description """2 - As bases de dados de saúde ou registos centralizados assentes nas plataformas únicas referidas
