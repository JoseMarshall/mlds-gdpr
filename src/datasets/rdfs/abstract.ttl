    eli:number "1" ;
    eli:realizes gdpr:p_3.cpt_3.art_56.pt_2.spt_1_abstract_de ;
    eli:title_alternative "Artikel 56-2-1" .

rgdpr:p_3.cpt_3.art_56.pt_2.spt_2_de a eli:LegalExpression ;
    eli:description "die öffentliche Sicherheit oder" ;
    eli:number "2" ;
    eli:realizes gdpr:p_3.cpt_3.art_56.pt_2.spt_2_abstract_de ;
    eli:title_alternative "Artikel 56-2-2" .

rgdpr:p_3.cpt_3.art_56.pt_2.spt_3_de a eli:LegalExpression ;
    eli:description "Rechtsgüter Dritter" ;
    eli:number "3" ;
    eli:realizes gdpr:p_3.cpt_3.art_56.pt_2.spt_3_abstract_de ;
    eli:title_alternative "Artikel 56-2-3" .

rgdpr:p_3.cpt_3.art_56.pt_2.spt_4_de a eli:LegalExpression ;
    eli:description "gefährdet würden, wenn das Interesse an der Vermeidung dieser Gefahren das Informationsinteresse der betroffenen Person überwiegt." ;
    eli:number "4" ;
    eli:realizes gdpr:p_3.cpt_3.art_56.pt_2.spt_4_abstract_de ;
    eli:title_alternative "Artikel 56-2-4" .

rgdpr:p_3.cpt_3.art_56.pt_2_de a eli:LegalExpression ;
    eli:description "In den Fällen des Absatzes 1 kann der Verantwortliche die Benachrichtigung insoweit und solange aufschieben, einschränken oder unterlassen, wie andernfalls" ;
    eli:number "2" ;
    eli:realizes gdpr:p_3.cpt_3.art_56.pt_2_abstract_de ;
    eli:title_alternative "Artikel 56-2" .

rgdpr:p_3.cpt_3.art_56.pt_3_de a eli:LegalExpression ;
    eli:description "Bezieht sich die Benachrichtigung auf die Übermittlung personenbezogener Daten an Verfassungsschutzbehörden, den Bundesnachrichtendienst, den Militärischen Abschirmdienst und, soweit die Sicherheit des Bundes berührt wird, andere Behörden des Bundesministeriums der Verteidigung, ist sie nur mit Zustimmung dieser Stellen zulässig." ;
    eli:number "3" ;
    eli:realizes gdpr:p_3.cpt_3.art_56.pt_3_abstract_de ;
    eli:title_alternative "Artikel 56-3" .

rgdpr:p_3.cpt_3.art_56.pt_4_de a eli:LegalExpression ;
    eli:description "Im Fall der Einschränkung nach Absatz 2 gilt Absatz 7 entsprechend." ;
    eli:number "4" ;
    eli:realizes gdpr:p_3.cpt_3.art_56.pt_4_abstract_de ;
    eli:title_alternative "Artikel 56-4" .

rgdpr:p_3.cpt_3.art_56_de a eli:LegalExpression ;
    eli:description "Benachrichtigung betroffener Personen" ;
    eli:number "56" ;
    eli:realizes gdpr:p_3.cpt_3.art_56_abstract_de ;
    eli:title_alternative "Artikel 56" .

rgdpr:p_3.cpt_3.art_57.pt_1.spt_1_de a eli:LegalExpression ;
