    eli:number "2" ;
    eli:realizes gdpr:cpt_6.sct_1.art_52.pt_2 ;
    eli:title_alternative "Article 52-2" .

rgdpr:cpt_6.sct_1.art_52.pt_2_eu_it a eli:LegalExpression ;
    eli:description """2.Nell'adempimento dei rispettivi compiti e nell'esercizio dei rispettivi poteri previsti dal
      presente regolamento, il membro o i membri di ogni autorità di controllo non subiscono
      pressioni esterne, né dirette, né indirette, e non sollecitano né accettano istruzioni da
      alcuno.""" ;
    eli:has_translation rgdpr:cpt_6.sct_1.art_52.pt_2_eu_de,
        rgdpr:cpt_6.sct_1.art_52.pt_2_eu_en,
        rgdpr:cpt_6.sct_1.art_52.pt_2_eu_pt ;
    eli:is_translation_of rgdpr:cpt_6.sct_1.art_52.pt_2_eu_de,
