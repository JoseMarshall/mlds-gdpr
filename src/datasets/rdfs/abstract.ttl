        rgdpr:cpt_6.sct_1.art_52.pt_3_eu_it,
        rgdpr:cpt_6.sct_1.art_52.pt_3_eu_pt ;
    eli:is_translation_of rgdpr:cpt_6.sct_1.art_52.pt_3_eu_de,
        rgdpr:cpt_6.sct_1.art_52.pt_3_eu_it,
        rgdpr:cpt_6.sct_1.art_52.pt_3_eu_pt ;
    eli:number "3" ;
    eli:realizes gdpr:cpt_6.sct_1.art_52.pt_3 ;
    eli:title_alternative "Article 52-3" .

rgdpr:cpt_6.sct_1.art_52.pt_3_eu_it a eli:LegalExpression ;
    eli:description """3.Il membro o i membri dell'autorità di controllo si astengono da qualunque azione
      incompatibile con le loro funzioni e per tutta la durata del mandato non possono esercitare
      alcuna altra attività incompatibile, remunerata o meno.""" ;
