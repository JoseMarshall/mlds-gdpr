        rgdpr:cpt_7.sct_1.art_60.pt_11_eu_it ;
    eli:number "11" ;
    eli:realizes gdpr:cpt_7.sct_1.art_60.pt_11 ;
    eli:title_alternative "Artigo 60.o-11" .

rgdpr:cpt_7.sct_1.art_60.pt_12_eu_de a eli:LegalExpression ;
