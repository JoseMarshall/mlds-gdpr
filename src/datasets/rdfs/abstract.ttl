    eli:number "1" ;
    eli:realizes gdpr:cpt_8.art_81.pt_1 ;
    eli:title_alternative "Article 81-1" .

rgdpr:cpt_8.art_81.pt_1_eu_it a eli:LegalExpression ;
    eli:description """1.L'autorità giurisdizionale competente di uno Stato membro che venga a conoscenza di azioni
      riguardanti lo stesso oggetto relativamente al trattamento dello stesso titolare del
      trattamento o dello stesso responsabile del trattamento pendenti presso un'autorità
      giurisdizionale in un altro Stato membro, prende contatto con tale autorità giurisdizionale
      nell'altro Stato membro per confermare l'esistenza delle azioni.""" ;
    eli:has_translation rgdpr:cpt_8.art_81.pt_1_eu_de,
        rgdpr:cpt_8.art_81.pt_1_eu_en,
        rgdpr:cpt_8.art_81.pt_1_eu_pt ;
