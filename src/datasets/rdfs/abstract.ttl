    eli:number "3" ;
    eli:realizes gdpr:cpt_4.sct_1.art_25.pt_3 ;
    eli:title_alternative "Article 25-3" .

rgdpr:cpt_4.sct_1.art_25.pt_3_eu_it a eli:LegalExpression ;
    eli:description """3.Un meccanismo di certificazione approvato ai sensi dell'articolo 42 può essere utilizzato
      come elemento per dimostrare la conformità ai requisiti di cui ai paragrafi 1 e 2 del presente
      articolo.""" ;
    eli:has_translation rgdpr:cpt_4.sct_1.art_25.pt_3_eu_de,
        rgdpr:cpt_4.sct_1.art_25.pt_3_eu_en,
        rgdpr:cpt_4.sct_1.art_25.pt_3_eu_pt ;
    eli:is_translation_of rgdpr:cpt_4.sct_1.art_25.pt_3_eu_de,
        rgdpr:cpt_4.sct_1.art_25.pt_3_eu_en,
        rgdpr:cpt_4.sct_1.art_25.pt_3_eu_pt ;
    eli:number "3" ;
    eli:realizes gdpr:cpt_4.sct_1.art_25.pt_3 ;
    eli:title_alternative "Articolo 25-3" .

rgdpr:cpt_4.sct_1.art_25.pt_3_eu_pt a eli:LegalExpression ;
    eli:description """3. Pode ser utilizado como elemento para demonstrar o cumprimento das obrigações estabelecidas nos
  n.os 1 e 2 do presente artigo, um procedimento de certificação aprovado
  nos termos do artigo 42.o.""" ;
    eli:has_translation rgdpr:cpt_4.sct_1.art_25.pt_3_eu_de,
        rgdpr:cpt_4.sct_1.art_25.pt_3_eu_en,
        rgdpr:cpt_4.sct_1.art_25.pt_3_eu_it ;
    eli:is_translation_of rgdpr:cpt_4.sct_1.art_25.pt_3_eu_de,
        rgdpr:cpt_4.sct_1.art_25.pt_3_eu_en,
        rgdpr:cpt_4.sct_1.art_25.pt_3_eu_it ;
    eli:number "3" ;
    eli:realizes gdpr:cpt_4.sct_1.art_25.pt_3 ;
    eli:title_alternative "Artigo 25.o-3" .

rgdpr:cpt_4.sct_1.art_25_eu_de a eli:LegalExpression ;
    eli:description "Datenschutz durch Technikgestaltung und durch datenschutzfreundliche Voreinstellungen" ;
    eli:has_translation rgdpr:cpt_4.sct_1.art_25_eu_en,
