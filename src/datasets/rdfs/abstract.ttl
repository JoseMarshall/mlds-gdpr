    eli:title_alternative "Artikel 46-14-2" .

rgdpr:p_3.cpt_1.art_46.pt_14.spt_3_de a eli:LegalExpression ;
    eli:description "biometrische Daten zur eindeutigen Identifizierung einer natürlichen Person," ;
