    eli:realizes gdpr:cpt_7.sct_2.art_66.pt_3 ;
    eli:title_alternative "Artigo 66.o-3" .

rgdpr:cpt_7.sct_2.art_66.pt_4_eu_de a eli:LegalExpression ;
