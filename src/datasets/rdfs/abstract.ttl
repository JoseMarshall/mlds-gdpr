    eli:title_alternative "Artikel 64-3-1" .

rgdpr:p_3.cpt_4.art_64.pt_3.spt_2_de a eli:LegalExpression ;
    eli:description "Verhinderung des unbefugten Lesens, Kopierens, Veränderns oder Löschens von Datenträgern (Datenträgerkontrolle)," ;
    eli:number "2" ;
