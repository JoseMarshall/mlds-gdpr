    eli:title_alternative "Artikel88-2" .

rgdpr:cpt_9.art_88.pt_2_eu_en a eli:LegalExpression ;
    eli:description "2.Those rules shall include suitable and specific measures to safeguard the data subject's human dignity, legitimate interests and fundamental rights, with particular regard to the transparency of processing, the transfer of personal data within a group of undertakings, or a group of enterprises engaged in a joint economic activity and monitoring systems at the work place." ;
    eli:has_translation rgdpr:cpt_9.art_88.pt_2_eu_de,
        rgdpr:cpt_9.art_88.pt_2_eu_it,
        rgdpr:cpt_9.art_88.pt_2_eu_pt ;
