    eli:title_alternative "Artikel 64-3-13" .

rgdpr:p_3.cpt_4.art_64.pt_3.spt_14_de a eli:LegalExpression ;
    eli:description "Gewährleistung, dass zu unterschiedlichen Zwecken erhobene personenbezogene Daten getrennt verarbeitet werden können (Trennbarkeit)." ;
    eli:number "14" ;
    eli:realizes gdpr:p_3.cpt_4.art_64.pt_3.spt_14_abstract_de ;
