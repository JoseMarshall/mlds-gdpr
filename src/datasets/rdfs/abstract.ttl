    eli:number "5" ;
    eli:realizes gdpr:cpt_6.art_29.pt_5_abstract_pt ;
    eli:title_alternative "Artigo 29.º-5" .

rgdpr:cpt_6.art_29.pt_6_pt a eli:LegalExpression ;
