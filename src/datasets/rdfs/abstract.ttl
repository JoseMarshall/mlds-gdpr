    eli:title_alternative "Artikel 55-3" .

rgdpr:p_3.cpt_3.art_55.pt_4_de a eli:LegalExpression ;
    eli:description "das Recht, die Bundesbeauftragte oder den Bundesbeauftragten anzurufen, und" ;
