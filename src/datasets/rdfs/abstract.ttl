    eli:realizes gdpr:cpt_4.sct_5.art_43.pt_6 ;
    eli:title_alternative "Articolo 43-6" .

rgdpr:cpt_4.sct_5.art_43.pt_6_eu_pt a eli:LegalExpression ;
