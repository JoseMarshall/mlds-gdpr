        rgdpr:cpt_7.sct_2.art_65.pt_3_eu_it ;
    eli:number "3" ;
    eli:realizes gdpr:cpt_7.sct_2.art_65.pt_3_eu_pt ;
    eli:title_alternative "Artigo 65.o-3" .

