rgdpr:p_2.cpt_1.sct_2.art_31.pt_1.spt_2_de a eli:LegalExpression ;
    eli:description "die zur Berechnung des Wahrscheinlichkeitswerts genutzten Daten unter Zugrundelegung eines wissenschaftlich anerkannten mathematisch-statistischen Verfahrens nachweisbar für die Berechnung der Wahrscheinlichkeit des bestimmten Verhaltens erheblich sind," ;
    eli:number "2" ;
