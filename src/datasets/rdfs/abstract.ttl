    eli:description "die Zwecke der von ihm vorgenommenen Verarbeitungen," ;
    eli:number "1" ;
    eli:realizes gdpr:p_3.cpt_3.art_55.pt_1_abstract_de ;
    eli:title_alternative "Artikel 55-1" .

rgdpr:p_3.cpt_3.art_55.pt_2_de a eli:LegalExpression ;
