        rgdpr:cpt_4.sct_5.art_42.pt_5_eu_pt ;
    eli:number "5" ;
    eli:realizes gdpr:cpt_4.sct_5.art_42.pt_5 ;
    eli:title_alternative "Article 42-5" .

rgdpr:cpt_4.sct_5.art_42.pt_5_eu_it a eli:LegalExpression ;
    eli:description """5.La certificazione ai sensi del presente articolo è rilasciata dagli organismi di
      certificazione di cui all'articolo 43 o dall'autorità di controllo competente in base ai
      criteri approvati da tale autorità di controllo competente ai sensi dell'articolo 58,
      paragrafo 3, o dal comitato, ai sensi dell'articolo 63. Ove i criteri siano approvati dal
      comitato, ciò può risultare in una certificazione comune, il sigillo europeo per la protezione
      dei dati.""" ;
    eli:has_translation rgdpr:cpt_4.sct_5.art_42.pt_5_eu_de,
        rgdpr:cpt_4.sct_5.art_42.pt_5_eu_en,
