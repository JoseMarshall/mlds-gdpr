        rgdpr:cpt_6.sct_1.art_52.pt_3_eu_en,
        rgdpr:cpt_6.sct_1.art_52.pt_3_eu_it ;
    eli:number "3" ;
    eli:realizes gdpr:cpt_6.sct_1.art_52.pt_3 ;
    eli:title_alternative "Artigo 52.o-3" .

rgdpr:cpt_6.sct_1.art_52.pt_4_eu_de a eli:LegalExpression ;
    eli:description """(4)Jeder Mitgliedstaat stellt sicher, dass jede Aufsichtsbehörde mit den personellen, technischen
  und finanziellen Ressourcen, Räumlichkeiten und Infrastrukturen ausgestattet wird, die sie
  benötigt, um ihre Aufgaben und Befugnisse auch im Rahmen der Amtshilfe, Zusammenarbeit und
  Mitwirkung im Ausschuss effektiv wahrnehmen zu können.""" ;
    eli:has_translation rgdpr:cpt_6.sct_1.art_52.pt_4_eu_en,
        rgdpr:cpt_6.sct_1.art_52.pt_4_eu_it,
        rgdpr:cpt_6.sct_1.art_52.pt_4_eu_pt ;
    eli:is_translation_of rgdpr:cpt_6.sct_1.art_52.pt_4_eu_en,
        rgdpr:cpt_6.sct_1.art_52.pt_4_eu_it,
        rgdpr:cpt_6.sct_1.art_52.pt_4_eu_pt ;
    eli:number "4" ;
    eli:realizes gdpr:cpt_6.sct_1.art_52.pt_4 ;
    eli:title_alternative "Artikel52-4" .

