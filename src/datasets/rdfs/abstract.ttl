    eli:realizes gdpr:p_3.cpt_4.art_64.pt_3.spt_3_abstract_de ;
    eli:title_alternative "Artikel 64-3-3" .

rgdpr:p_3.cpt_4.art_64.pt_3.spt_4_de a eli:LegalExpression ;
