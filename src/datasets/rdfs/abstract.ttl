    eli:realizes gdpr:cpt_5.art_45.pt_2.spt_a_eu_en ;
    eli:title_alternative "Article 45-2-(a)" .

rgdpr:cpt_5.art_45.pt_2.spt_a_eu_it a eli:LegalExpression ;
    eli:description """lo stato di diritto, il rispetto dei diritti umani e delle libert fondamentali, la
              pertinente legislazione generale e settoriale (anche in materia di sicurezza pubblica,
