    eli:number "2" ;
    eli:realizes gdpr:p_3.cpt_3.art_55.pt_2_abstract_de ;
    eli:title_alternative "Artikel 55-2" .

rgdpr:p_3.cpt_3.art_55.pt_3_de a eli:LegalExpression ;
