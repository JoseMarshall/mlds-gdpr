        rgdpr:cpt_6.sct_1.art_51.pt_1_eu_it,
        rgdpr:cpt_6.sct_1.art_51.pt_1_eu_pt ;
    eli:is_translation_of rgdpr:cpt_6.sct_1.art_51.pt_1_eu_de,
        rgdpr:cpt_6.sct_1.art_51.pt_1_eu_it,
