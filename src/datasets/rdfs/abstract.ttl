    eli:title_alternative "Artikel 64-3-11" .

rgdpr:p_3.cpt_4.art_64.pt_3.spt_12_de a eli:LegalExpression ;
    eli:description "Gewährleistung, dass personenbezogene Daten, die im Auftrag verarbeitet werden, nur entsprechend den Weisungen des Auftraggebers verarbeitet werden können (Auftragskontrolle)," ;
    eli:number "12" ;
    eli:realizes gdpr:p_3.cpt_4.art_64.pt_3.spt_12_abstract_de ;
    eli:title_alternative "Artikel 64-3-12" .

rgdpr:p_3.cpt_4.art_64.pt_3.spt_13_de a eli:LegalExpression ;
    eli:description "Gewährleistung, dass personenbezogene Daten gegen Zerstörung oder Verlust geschützt sind (Verfügbarkeitskontrolle)," ;
