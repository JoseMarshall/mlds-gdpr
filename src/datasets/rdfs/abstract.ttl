rgdpr:cpt_6.sct_1.art_52.pt_2_eu_pt a eli:LegalExpression ;
    eli:description """2. Os membros das autoridades de controlo não estão sujeitos a influências externas, diretas ou
  indiretas no desempenho das suas funções e no exercício dos seus poderes nos termos do presente
  regulamento, e não solicitam nem recebem instruções de outrem.""" ;
    eli:has_translation rgdpr:cpt_6.sct_1.art_52.pt_2_eu_de,
        rgdpr:cpt_6.sct_1.art_52.pt_2_eu_en,
        rgdpr:cpt_6.sct_1.art_52.pt_2_eu_it ;
