    eli:is_translation_of rgdpr:cpt_7.sct_1.art_62.pt_7_eu_de,
        rgdpr:cpt_7.sct_1.art_62.pt_7_eu_en,
        rgdpr:cpt_7.sct_1.art_62.pt_7_eu_pt ;
