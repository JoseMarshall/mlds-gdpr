rgdpr:p_3.cpt_3.art_56.pt_1_de a eli:LegalExpression ;
    eli:description "Ist die Benachrichtigung betroffener Personen über die Verarbeitung sie betreffender personenbezogener Daten in speziellen Rechtsvorschriften, insbesondere bei verdeckten Maßnahmen, vorgesehen oder angeordnet, so hat diese Benachrichtigung zumindest die folgenden Angaben zu enthalten:" ;
    eli:number "1" ;
    eli:realizes gdpr:p_3.cpt_3.art_56.pt_1_abstract_de ;
