        rgdpr:cpt_8.art_81.pt_2_eu_pt ;
    eli:number "2" ;
    eli:realizes gdpr:cpt_8.art_81.pt_2 ;
    eli:title_alternative "Artikel 81-2" .

rgdpr:cpt_8.art_81.pt_2_eu_en a eli:LegalExpression ;
    eli:description "2.Where proceedings concerning the same subject matter as regards processing of the same controller or processor are pending in a court in another MemberState, any competent court other than the court first seized may suspend its proceedings." ;
    eli:has_translation rgdpr:cpt_8.art_81.pt_2_eu_de,
        rgdpr:cpt_8.art_81.pt_2_eu_it,
        rgdpr:cpt_8.art_81.pt_2_eu_pt ;
