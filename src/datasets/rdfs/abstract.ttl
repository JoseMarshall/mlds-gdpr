    eli:number "1" ;
    eli:realizes gdpr:cpt_5.art_45.pt_1 ;
    eli:title_alternative "Article 45-1" .

rgdpr:cpt_5.art_45.pt_1_eu_it a eli:LegalExpression ;
    eli:description """1.Il trasferimento di dati personali verso un paese terzo o un'organizzazione internazionale è
      ammesso se la Commissione ha deciso che il paese terzo, un territorio o uno o più settori
      specifici all'interno del paese terzo, o l'organizzazione internazionale in questione
      garantiscono un livello di protezione adeguato. In tal caso il trasferimento non necessita di
      autorizzazioni specifiche.""" ;
    eli:has_translation rgdpr:cpt_5.art_45.pt_1_eu_de,
        rgdpr:cpt_5.art_45.pt_1_eu_en,
        rgdpr:cpt_5.art_45.pt_1_eu_pt ;
