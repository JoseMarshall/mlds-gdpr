    eli:number "5" ;
    eli:realizes gdpr:cpt_7.sct_2.art_65.pt_5_eu_de ;
    eli:title_alternative "Artikel 65-5" .

rgdpr:cpt_7.sct_2.art_65.pt_5_eu_en a eli:LegalExpression ;
    eli:description "5.The Chair of the Board shall notify, without undue delay, the decision referred to in paragraph 1 to the supervisory authorities concerned. It shall inform the Commission thereof. The decision shall be published on the website of the Board without delay after the supervisory authority has notified the final decision referred to in paragraph6." ;
    eli:has_translation rgdpr:cpt_7.sct_2.art_65.pt_5_eu_de,
        rgdpr:cpt_7.sct_2.art_65.pt_5_eu_it,
