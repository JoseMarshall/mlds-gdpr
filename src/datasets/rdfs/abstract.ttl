rgdpr:p_2.cpt_1.sct_2.art_31.pt_1.spt_4_de a eli:LegalExpression ;
    eli:description "im Fall der Nutzung von Anschriftendaten die betroffene Person vor Berechnung des Wahrscheinlichkeitswerts über die vorgesehene Nutzung dieser Daten unterrichtet worden ist; die Unterrichtung ist zu dokumentieren." ;
    eli:number "4" ;
