        rgdpr:cpt_7.sct_1.art_60.pt_7_eu_en,
        rgdpr:cpt_7.sct_1.art_60.pt_7_eu_it ;
    eli:is_part_of rgdpr:cpt_7.sct_1.art_60_eu_pt ;
