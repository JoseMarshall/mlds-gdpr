    eli:number "3" ;
    eli:realizes gdpr:cpt_6.art_31.pt_3_abstract_pt ;
    eli:title_alternative "Artigo 31.º-3" .

rgdpr:cpt_6.art_31.pt_4_pt a eli:LegalExpression ;
    eli:description """4 - O consentimento relativo ao tratamento de dados para fins de investigação científica pode
