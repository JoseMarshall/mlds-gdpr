    eli:number "c" ;
    eli:realizes gdpr:p_2.cpt_2.art_33.pt_1.spt_1.sspt_c_abstract_de ;
    eli:title_alternative "Artikel 33-1-1-c" .

rgdpr:p_2.cpt_2.art_33.pt_1.spt_1_de a eli:LegalExpression ;
    eli:description "im Fall einer öffentlichen Stelle" ;
