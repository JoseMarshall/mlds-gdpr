    eli:realizes gdpr:cpt_6.sct_2.art_55.pt_3 ;
    eli:title_alternative "Article 55-3" .

rgdpr:cpt_6.sct_2.art_55.pt_3_eu_it a eli:LegalExpression ;
    eli:description """3.Le autorità di controllo non sono competenti per il controllo dei trattamenti effettuati
      dalle autorità giurisdizionali nell'esercizio delle loro funzioni giurisdizionali.""" ;
    eli:has_translation rgdpr:cpt_6.sct_2.art_55.pt_3_eu_de,
        rgdpr:cpt_6.sct_2.art_55.pt_3_eu_en,
        rgdpr:cpt_6.sct_2.art_55.pt_3_eu_pt ;
    eli:is_translation_of rgdpr:cpt_6.sct_2.art_55.pt_3_eu_de,
        rgdpr:cpt_6.sct_2.art_55.pt_3_eu_en,
        rgdpr:cpt_6.sct_2.art_55.pt_3_eu_pt ;
    eli:number "3" ;
    eli:realizes gdpr:cpt_6.sct_2.art_55.pt_3 ;
    eli:title_alternative "Articolo 55-3" .

rgdpr:cpt_6.sct_2.art_55.pt_3_eu_pt a eli:LegalExpression ;
    eli:description """3. As autoridades de controlo não têm competência para controlar operações de tratamento efetuadas
  por tribunais que atuem no exercício da sua função jurisdicional.""" ;
    eli:has_translation rgdpr:cpt_6.sct_2.art_55.pt_3_eu_de,
        rgdpr:cpt_6.sct_2.art_55.pt_3_eu_en,
