    eli:title_alternative "Artikel 80-2" .

rgdpr:cpt_8.art_80.pt_2_eu_en a eli:LegalExpression ;
    eli:description "2.Member States may provide that any body, organisation or association referred to in paragraph1 of this Article, independently of a data subject's mandate, has the right to lodge, in that Member State, a complaint with the supervisory authority which is competent pursuant to Article 77 and to exercise the rights referred to in Articles78 and79 if it considers that the rights of a data subject under this Regulation have been infringed as a result of the processing." ;
    eli:has_translation rgdpr:cpt_8.art_80.pt_2_eu_de,
        rgdpr:cpt_8.art_80.pt_2_eu_it,
