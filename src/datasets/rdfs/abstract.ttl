rgdpr:cpt_6.sct_1.art_52.pt_1_eu_en a eli:LegalExpression ;
    eli:description "1.Each supervisory authority shall act with complete independence in performing its tasks and exercising its powers in accordance with this Regulation." ;
    eli:has_translation rgdpr:cpt_6.sct_1.art_52.pt_1_eu_de,
