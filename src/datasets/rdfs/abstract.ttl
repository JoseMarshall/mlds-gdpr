    eli:realizes gdpr:p_3.cpt_3.art_57.pt_1.spt_1_abstract_de ;
    eli:title_alternative "Artikel 57-1-1" .

rgdpr:p_3.cpt_3.art_57.pt_1.spt_2_de a eli:LegalExpression ;
    eli:description "die verfügbaren Informationen über die Herkunft der Daten," ;
    eli:number "2" ;
    eli:realizes gdpr:p_3.cpt_3.art_57.pt_1.spt_2_abstract_de ;
    eli:title_alternative "Artikel 57-1-2" .

rgdpr:p_3.cpt_3.art_57.pt_1.spt_3_de a eli:LegalExpression ;
    eli:description "die Zwecke der Verarbeitung und deren Rechtsgrundlage," ;
    eli:number "3" ;
    eli:realizes gdpr:p_3.cpt_3.art_57.pt_1.spt_3_abstract_de ;
    eli:title_alternative "Artikel 57-1-3" .

rgdpr:p_3.cpt_3.art_57.pt_1.spt_4_de a eli:LegalExpression ;
    eli:description "die Empfänger oder die Kategorien von Empfängern, gegenüber denen die Daten offengelegt worden sind, insbesondere bei Empfängern in Drittstaaten oder bei internationalen Organisationen," ;
