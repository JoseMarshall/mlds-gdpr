    eli:title_alternative "Artikel 81-3" .

rgdpr:cpt_8.art_81.pt_3_eu_en a eli:LegalExpression ;
    eli:description "3.Where those proceedings are pending at first instance, any court other than the court first seized may also, on the application of one of the parties, decline jurisdiction if the court first seized has jurisdiction over the actions in question and its law permits the consolidation thereof." ;
    eli:has_translation rgdpr:cpt_8.art_81.pt_3_eu_de,
        rgdpr:cpt_8.art_81.pt_3_eu_it,
        rgdpr:cpt_8.art_81.pt_3_eu_pt ;
