    eli:is_translation_of rgdpr:cpt_7.sct_2.art_65.pt_5_eu_de,
        rgdpr:cpt_7.sct_2.art_65.pt_5_eu_it,
        rgdpr:cpt_7.sct_2.art_65.pt_5_eu_pt ;
    eli:number "5" ;
    eli:realizes gdpr:cpt_7.sct_2.art_65.pt_5_eu_en ;
    eli:title_alternative "Article 65-5" .

rgdpr:cpt_7.sct_2.art_65.pt_5_eu_it a eli:LegalExpression ;
    eli:description """5.Il presidente del comitato notifica senza ingiustificato ritardo alle autorit di controllo
      interessate la decisione di cui al paragrafo 1 e ne informa la Commissione. La decisione 
      pubblicata senza ritardo sul sito web del comitato dopo che l'autorit di controllo ha
      notificato la decisione definitiva di cui al paragrafo 6.""" ;
    eli:has_translation rgdpr:cpt_7.sct_2.art_65.pt_5_eu_de,
        rgdpr:cpt_7.sct_2.art_65.pt_5_eu_en,
