        rgdpr:cpt_4.sct_5.art_42.pt_5_eu_en,
        rgdpr:cpt_4.sct_5.art_42.pt_5_eu_pt ;
    eli:number "5" ;
    eli:realizes gdpr:cpt_4.sct_5.art_42.pt_5 ;
    eli:title_alternative "Articolo 42-5" .

rgdpr:cpt_4.sct_5.art_42.pt_5_eu_pt a eli:LegalExpression ;
    eli:description """5. A certificação prevista no presente artigo é emitida pelos organismos de certificação referidos
  no artigo 43.o ou pela autoridade de controlo competente, com base nos
  critérios por esta aprovados por força do artigo 58.o, n.o
  3, ou pelo Comité por força do artigo 63.o. Caso os critérios sejam
  aprovados pelo Comité, podem ter como resultado uma certificação comum, o Selo Europeu de Proteção
  de Dados.""" ;
    eli:has_translation rgdpr:cpt_4.sct_5.art_42.pt_5_eu_de,
        rgdpr:cpt_4.sct_5.art_42.pt_5_eu_en,
