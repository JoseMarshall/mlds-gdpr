    eli:title_alternative "Artikel 65-3-3" .

rgdpr:p_3.cpt_4.art_65.pt_3.spt_4_de a eli:LegalExpression ;
    eli:description "eine Beschreibung der von dem Verantwortlichen ergriffenen oder vorgeschlagenen Maßnahmen zur Behandlung der Verletzung und der getroffenen Maßnahmen zur Abmilderung ihrer möglichen nachteiligen Auswirkungen." ;
    eli:number "4" ;
    eli:realizes gdpr:p_3.cpt_4.art_65.pt_3.spt_4_abstract_de ;
    eli:title_alternative "Artikel 65-3-4" .

rgdpr:p_3.cpt_4.art_65.pt_3_de a eli:LegalExpression ;
    eli:description "Die Meldung nach Absatz 1 hat zumindest folgende Informationen zu enthalten:" ;
    eli:number "3" ;
    eli:realizes gdpr:p_3.cpt_4.art_65.pt_3_abstract_de ;
    eli:title_alternative "Artikel 65-3" .

rgdpr:p_3.cpt_4.art_65.pt_4_de a eli:LegalExpression ;
