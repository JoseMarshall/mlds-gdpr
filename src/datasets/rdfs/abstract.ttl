rgdpr:cpt_6.sct_2.art_55.pt_3_eu_en a eli:LegalExpression ;
    eli:description "3.Supervisory authorities shall not be competent to supervise processing operations of courts acting in their judicial capacity." ;
    eli:has_translation rgdpr:cpt_6.sct_2.art_55.pt_3_eu_de,
        rgdpr:cpt_6.sct_2.art_55.pt_3_eu_it,
