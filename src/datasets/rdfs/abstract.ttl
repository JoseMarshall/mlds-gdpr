  RGPD.""" ;
    eli:number "2" ;
    eli:realizes gdpr:cpt_6.art_30.pt_2_abstract_pt ;
    eli:title_alternative "Artigo 30.º-2" .

rgdpr:cpt_6.art_31.pt_1_pt a eli:LegalExpression ;
    eli:description """1 - O tratamento para fins de arquivo de interesse público, fins de investigação científica ou
