        rgdpr:cpt_6.sct_1.art_52.pt_3_eu_en,
        rgdpr:cpt_6.sct_1.art_52.pt_3_eu_pt ;
    eli:is_translation_of rgdpr:cpt_6.sct_1.art_52.pt_3_eu_de,
        rgdpr:cpt_6.sct_1.art_52.pt_3_eu_en,
        rgdpr:cpt_6.sct_1.art_52.pt_3_eu_pt ;
    eli:number "3" ;
    eli:realizes gdpr:cpt_6.sct_1.art_52.pt_3 ;
    eli:title_alternative "Articolo 52-3" .

rgdpr:cpt_6.sct_1.art_52.pt_3_eu_pt a eli:LegalExpression ;
    eli:description """3. Os membros da autoridade de controlo abstêm-se de qualquer ato incompatível com as suas funções
  e, durante o seu mandato, não podem desempenhar nenhuma atividade, remunerada ou não, que com elas
