rgdpr:cpt_7.sct_1.art_60.pt_8_eu_en a eli:LegalExpression ;
    eli:description "8.By derogation from paragraph 7, where a complaint is dismissed or rejected, the supervisory authority with which the complaint was lodged shall adopt the decision and notify it to the complainant and shall inform the controller thereof." ;
    eli:has_translation rgdpr:cpt_7.sct_1.art_60.pt_8_eu_de,
