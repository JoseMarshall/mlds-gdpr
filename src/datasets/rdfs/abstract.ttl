    eli:realizes gdpr:p_3.cpt_4.art_67.pt_3_abstract_de ;
    eli:title_alternative "Artikel 67-3" .

rgdpr:p_3.cpt_4.art_67.pt_4.spt_1_de a eli:LegalExpression ;
    eli:description "eine systematische Beschreibung der geplanten Verarbeitungsvorgänge und der Zwecke der Verarbeitung," ;
