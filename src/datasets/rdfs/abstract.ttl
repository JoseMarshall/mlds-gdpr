    eli:title_alternative "Article 52-1" .

rgdpr:cpt_6.sct_1.art_52.pt_1_eu_it a eli:LegalExpression ;
    eli:description """1.Ogni autorità di controllo agisce in piena indipendenza nell'adempimento dei propri compiti
      e nell'esercizio dei propri poteri conformemente al presente regolamento.""" ;
    eli:has_translation rgdpr:cpt_6.sct_1.art_52.pt_1_eu_de,
