rgdpr:cpt_8.art_81.pt_1_eu_en a eli:LegalExpression ;
    eli:description "1.Where a competent court of a Member State has information on proceedings, concerning the same subject matter as regards processing by the same controller or processor, that are pending in a court in another MemberState, it shall contact that court in the other MemberState to confirm the existence of such proceedings." ;
    eli:has_translation rgdpr:cpt_8.art_81.pt_1_eu_de,
        rgdpr:cpt_8.art_81.pt_1_eu_it,
