        rgdpr:cpt_6.sct_2.art_55.pt_3_eu_en,
        rgdpr:cpt_6.sct_2.art_55.pt_3_eu_it ;
    eli:number "3" ;
    eli:realizes gdpr:cpt_6.sct_2.art_55.pt_3 ;
    eli:title_alternative "Artigo 55.o-3" .

